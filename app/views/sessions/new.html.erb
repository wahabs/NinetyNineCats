<h1>Log in!</h1>

<form action="/session" method="POST">

  <input type="hidden"
  name="authenticity_token"
  value="<%= form_authenticity_token %>">
  

  <label for="user_user_name">Username</label>
  <input
  type="text"
  name="user[user_name]"
  id="user_user_name">
  <br>

  <label for="user_password">Password</label>
  <input
  type="password"
  name="user[password]"
  id="user_password">
  <br>

  <input type="submit" value="Log in">

</form>
